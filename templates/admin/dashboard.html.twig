{% extends 'base.html.twig' %}

{% block body %}
    <ul class="list-group">
        {% for user in users %}
            <li class="list-group-item">
                <a href="{{ path('edit_user', { 'id': user.id }) }}">{{ user.username }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
